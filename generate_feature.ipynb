    "def lcs(a,b):\n",
    "    lena=len(a)\n",
    "    lenb=len(b)\n",
    "    c=[[0 for i in range(lenb+1)] for j in range(lena+1)]\n",
    "    for i in range(lena):\n",
    "        for j in range(lenb):\n",
    "            if a[i]==b[j]:\n",
    "                c[i+1][j+1]=c[i][j]+1\n",
    "            elif c[i+1][j]>c[i][j+1]:\n",
    "                c[i+1][j+1]=c[i+1][j]\n",
    "            else:\n",
    "                c[i+1][j+1]=c[i][j+1]\n",
    "    return c[lena][lenb]"
